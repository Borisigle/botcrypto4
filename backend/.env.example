BINANCE_WS_BASE_URL=wss://fstream.binance.com/ws
BINANCE_REST_BASE_URL=https://fapi.binance.com
DEPTH_SNAPSHOT_LIMIT=500
LOG_LEVEL=INFO
METRICS_WINDOW_SEC=60
